  <itemDefinition id="ItemDefinition_2" structureRef="life.genny.rules.QRules" />
  <process processType="Private" isExecutable="true" id="com.sample.bpmn.hello" name="Hello World" >

    <extensionElements>
     <tns:import name="life.genny.rules.QRules" />
    </extensionElements>
    <!-- process variables -->
    <property id="rules" itemSubjectRef="ItemDefinition_2"/>

    <!-- nodes -->
    <scriptTask id="_2" name="Hello" >
      <script>System.out.println("Hello  Anish");</script>
    </scriptTask>
    <startEvent id="_1"  isInterrupting="true">
    </startEvent>
    <endEvent id="_3" >
        <terminateEventDefinition />
    </endEvent>
    <scriptTask id="_jbpm-unique-1" name="Script" >
      <script>System.out.println("Adios Anish!");</script>
    </scriptTask>
    <scriptTask id="ScriptTask_1" name="Script Task 1" scriptFormat="http://www.java.com/java" >
      <script>System.out.println("print 3rd task");
QRules rules2 = (QRules)kcontext.getVariable("rules");
System.out.println("QRules realm = "+rules2.realm());</script>
    </scriptTask>

    <!-- connections -->
    <sequenceFlow id="_1-_2" sourceRef="_1" targetRef="_2" />
    <sequenceFlow id="ScriptTask_1-_3" sourceRef="ScriptTask_1" targetRef="_3" />
    <sequenceFlow id="_2-_jbpm-unique-1" sourceRef="_2" targetRef="_jbpm-unique-1" />
    <sequenceFlow id="_jbpm-unique-1-ScriptTask_1" sourceRef="_jbpm-unique-1" targetRef="ScriptTask_1" />

  </process>

