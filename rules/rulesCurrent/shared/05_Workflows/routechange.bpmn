    <bpmn2:scriptTask id="ScriptTask_2" name="Route change script" scriptFormat="http://www.java.com/java">
      <bpmn2:extensionElements>
        <tns:metaData name="elementname">
          <tns:metaValue><![CDATA[Route change script]]></tns:metaValue>
        </tns:metaData>
      </bpmn2:extensionElements>
      <bpmn2:incoming>SequenceFlow_3</bpmn2:incoming>
      <bpmn2:outgoing>SequenceFlow_4</bpmn2:outgoing>
      <bpmn2:script>
	Object obj = rules.getAsObject(&quot;m&quot;);
	QEventMessage m =(QEventMessage) obj;
	rules.println(&quot;RULE INIT ROUTE_CHANGE: &quot;+m);

        rules.setState(&quot;ROUTE_CHANGE_LOOP&quot;);

        String route = m.getData().getCode();
        System.out.println(route);
        String selectedItem = m.getData().getValue();

        if(route != null) {
          rules.setState(route);
          rules.set(&quot;routeValue&quot;, route);
        }

        if(selectedItem != null) {
           rules.set(&quot;selectedItem&quot;, selectedItem);
            rules.setState(selectedItem);
        }

        rules.setState(&quot;STARTED&quot;);
        rules.setState(&quot;EVENT_ROUTE_CHANGE&quot;);</bpmn2:script>
    </bpmn2:scriptTask>
