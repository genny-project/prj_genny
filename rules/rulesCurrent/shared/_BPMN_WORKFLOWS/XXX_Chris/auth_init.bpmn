    <bpmn2:scriptTask id="ScriptTask_14" name="Send Attributes" scriptFormat="http://www.java.com/java">
      <bpmn2:extensionElements>
        <drools:metaData name="elementname">
          <drools:metaValue><![CDATA[Send Attributes]]></drools:metaValue>
        </drools:metaData>
      </bpmn2:extensionElements>
      <bpmn2:incoming>SequenceFlow_1</bpmn2:incoming>
      <bpmn2:outgoing>SequenceFlow_4</bpmn2:outgoing>
      <bpmn2:script>kcontext.setVariable(&quot;currentUser&quot;,currentUser);

System.out.println(&quot;authInit: Sending Attributes&quot;);
		try {
			QDataAttributeMessage msg = RulesUtils.loadAllAttributesIntoCache(userToken.getToken());
			msg.setToken(userToken.getToken());
			VertxUtils.writeMsg(&quot;webcmds&quot;,JsonUtils.toJson(msg));
			System.out.println(&quot;authInit: All the attributes sent &quot; + msg.getItems().length + &quot; of them&quot;);

		} catch (Exception e) {
			System.out.println(&quot;authInit: No web browser to talk to!&quot;);
		}

System.out.println(&quot;authInit: Attributes sent&quot;);</bpmn2:script>
    </bpmn2:scriptTask>
    <bpmn2:scriptTask id="ScriptTask_1" name="SendProject" scriptFormat="http://www.java.com/java">
      <bpmn2:extensionElements>
        <drools:metaData name="elementname">
          <drools:metaValue><![CDATA[SendProject]]></drools:metaValue>
        </drools:metaData>
      </bpmn2:extensionElements>
      <bpmn2:incoming>SequenceFlow_4</bpmn2:incoming>
      <bpmn2:outgoing>SequenceFlow_2</bpmn2:outgoing>
      <bpmn2:script>System.out.println(&quot;authInit: Sending Projects...&quot;);

BaseEntityUtils beUtils = new BaseEntityUtils(userToken);
System.out.println(&quot;authInit: beUtils created&quot;);
String projectCode = &quot;PRJ_&quot;+userToken.getRealm().toUpperCase();
System.out.println(&quot;authInit:  Project Code = &quot;+projectCode);
BaseEntity project = beUtils.getBaseEntityByCode(projectCode);

System.out.println(&quot;authInit: Sending project = &quot;+project);
System.out.println(&quot;authInit: currentUser = &quot;+currentUser);
/* System.out.println(userToken.getToken()); */

VertxUtils.publish(currentUser,&quot;webcmds&quot;,project, &quot;PROJECT&quot;, userToken.getToken());

System.out.println(&quot;authInit: Projects sent&quot;);</bpmn2:script>
    </bpmn2:scriptTask>
