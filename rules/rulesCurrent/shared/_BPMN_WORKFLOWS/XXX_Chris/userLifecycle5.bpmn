      <bpmn2:outgoing>SequenceFlow_24</bpmn2:outgoing>
      <bpmn2:script>BaseEntityUtils beUtils = new BaseEntityUtils((GennyToken)userToken);
String payload = &quot;This is supposed to come through&quot;;
newUser = beUtils.create(((GennyToken)userToken).getUserCode(), ((GennyToken)userToken).getUserCode());

kcontext.setVariable(&quot;newUser&quot;,newUser);
kcontext.setVariable(&quot;userCode&quot;, ((GennyToken)userToken).getUserCode());

System.out.println(&quot;userLifecycle: New User Created =&quot; + newUser+&quot; now sending to channel callback :[&quot;+((GennyToken)userToken).getUserCode()+&quot;]&quot; );
System.out.println(&quot;userLifecycle: callingProcessId = &quot;+callingProcessId);
System.out.println(&quot;userLifecycle: callingProcessToken = &quot;+ callingProcessToken);
System.out.println(&quot;userLifecycle: currentUser = &quot;+ currentUser);</bpmn2:script>
