rule "FRM_LOGO"
    ruleflow-group 'GenerateFrames'

    when
      not  Frame3(code == "FRM_LOGO")
      serviceToken : GennyToken( code == "PER_SERVICE")
    then
    	System.out.println("   Generate FRM_LOGO Rule  "+serviceToken.getUserCode());

			Frame3 frame = Frame3.builder("FRM_LOGO")
										.addTheme("THM_TREE_ITEM", ThemePosition.WRAPPER, serviceToken).end()
										.addTheme("THM_MARGIN_BOTTOM_20", ThemePosition.WRAPPER, serviceToken).end()
										/*.addTheme("THM_LOGO_CENTRE", ThemePosition.CENTRE, serviceToken).end() */
										.question("QUE_PROJECT_SIDEBAR_GRP")
											.targetAlias("PRJ_"+serviceToken.getRealm().toUpperCase())
											.addTheme("THM_LOGO", serviceToken)
											.vcl(VisualControlType.INPUT_SELECTED).end()
										.end()
										.build();

			frame.setRealm(serviceToken.getRealm());

			insert (frame);

			/* Process and save to cache */
			FrameUtils2.toMessage(frame, serviceToken);
end
