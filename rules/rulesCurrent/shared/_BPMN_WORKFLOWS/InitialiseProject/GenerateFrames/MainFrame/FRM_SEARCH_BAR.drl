rule "FRM_SEARCH_BAR"
  ruleflow-group 'GenerateFrames'

  when
    not  Frame3(code == "FRM_SEARCH_BAR")
    serviceToken : GennyToken( code == "PER_SERVICE")

  then
    System.out.println(" Generate FRM_SEARCH_BAR Rule  "+serviceToken.getUserCode());

    Frame3 frame = Frame3.builder("FRM_SEARCH_BAR")
                  .addTheme("THM_SEARCH_BAR_CENTRE", ThemePosition.CENTRE, serviceToken).end()
                  .question("QUE_SEARCH")
                    .addTheme("THM_SEARCH_BAR", serviceToken).vcl(VisualControlType.VCL).end()
                    .addTheme("THM_SEARCH_BAR_WRAPPER", serviceToken).vcl(VisualControlType.VCL_WRAPPER).end()
                  .end()
                  .build();
    frame.setRealm(serviceToken.getRealm());
    insert (frame);

    /* Process and save to cache */
    FrameUtils2.toMessage(frame, serviceToken);
end
