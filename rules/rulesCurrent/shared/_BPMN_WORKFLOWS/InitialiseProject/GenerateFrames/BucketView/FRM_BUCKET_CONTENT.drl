rule "FRM_BUCKET_CONTENT"
    ruleflow-group 'GenerateFrames'

    when
      not  Frame3(code == "FRM_BUCKET_CONTENT")
      serviceToken : GennyToken( code == "PER_SERVICE")
      
    then
     System.out.println(" Generate FRM_BUCKET_CONTENT Rule  "+serviceToken.getUserCode());

			Frame3 frame = Frame3.builder("FRM_BUCKET_CONTENT")
										.question("QUE_BUCKET_CONTENT_GRP").end()
										.addTheme("THM_JUSTIFY_CONTENT_FLEX_START", ThemePosition.CENTRE, serviceToken).end()
										.addTheme("THM_PADDING_15", ThemePosition.WRAPPER, serviceToken).end()
										.build();
							
	frame.setRealm(serviceToken.getRealm());
						
	insert (frame);
	
	/* Process and save to cache */
	FrameUtils2.toMessage(frame, serviceToken);

end
