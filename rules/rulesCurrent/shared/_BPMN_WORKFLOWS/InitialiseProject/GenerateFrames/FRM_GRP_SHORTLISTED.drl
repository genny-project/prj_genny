rule "FRM_GRP_SHORTLISTED"
    ruleflow-group 'GenerateFrames'
  
    when
      not  Frame3(code == "FRM_GRP_SHORTLISTED")
      serviceToken : GennyToken( code == "PER_SERVICE")
      
    then
     System.out.println("   Generate FRM_GRP_SHORTLISTED Rule  "+serviceToken.getUserCode());

 	 Frame3 frame = Frame3.builder("FRM_GRP_SHORTLISTED")
                                .addTheme("THM_TEST1",serviceToken)
                                	.end()
                                .question("QUE_GRP_SHORTLISTED")
                                	.addTheme("THM_FORM_INPUT_DEFAULT")
                                		.vcl(VisualControlType.VCL_INPUT)
                                		.weight(2.0)
                                		.end()
                            		.end()
                                .addTheme("THM_BUCKET_LABEL",serviceToken)
                                	.end()
                                .build();
						
	frame.setRealm(serviceToken.getRealm());
						
	insert (frame);
	
	/* Process and save to cache */
	FrameUtils2.toMessage(frame, serviceToken);

end
