rule "FRM_BUCKET_COLUMN_3"
    ruleflow-group 'GenerateFrames'
  
    when
      not  Frame3(code == "FRM_BUCKET_COLUMN_3")
      serviceToken : GennyToken( code == "PER_SERVICE")
      FRM_GRP_SHORTLISTED : Frame3 (code == "FRM_GRP_SHORTLISTED" )
      FRM_BUCKET_COLUMN_2 : Frame3 (code == "FRM_BUCKET_COLUMN_2" ) /* just to see a sequence */
    then
     System.out.println("   Generate FRM_BUCKET_COLUMN_3 Rule  "+serviceToken.getUserCode());

 	 Frame3 frame = Frame3.builder("FRM_BUCKET_COLUMN_3")
                                .addTheme("THM_BUCKET_COLUMN",serviceToken).end()
                                .addFrame(FRM_GRP_SHORTLISTED, FramePosition.NORTH).end()
                                .build();
						
	frame.setRealm(serviceToken.getRealm());
						
	insert (frame);
	
	/* Process and save to cache */
	FrameUtils2.toMessage(frame, serviceToken);

end
