rule "FRM_DASHBOARD_INTERNSHIPS_CONTENT"
    ruleflow-group 'GenerateFrames'

    when
      not  Frame3(code == "FRM_DASHBOARD_INTERNSHIPS_CONTENT")
      serviceToken : GennyToken( code == "PER_SERVICE")
      FRM_COUNT_ALL_INTERNSHIPS : Frame3 ( code == "FRM_COUNT_ALL_INTERNSHIPS" )
      FRM_COUNT_AVAILABLE_INTERNSHIPS  : Frame3 ( code == "FRM_COUNT_AVAILABLE_INTERNSHIPS" )
      FRM_COUNT_APPLIED_SHORTLISTED_INTERVIEWED_INTERNSHIPS : Frame3 ( code == "FRM_COUNT_APPLIED_SHORTLISTED_INTERVIEWED_INTERNSHIPS" )
      FRM_COUNT_OFFERED_INTERNSHIPS : Frame3 ( code == "FRM_COUNT_OFFERED_INTERNSHIPS" )
      FRM_COUNT_PLACED_INTERNSHIPS   : Frame3 ( code == "FRM_COUNT_PLACED_INTERNSHIPS" )
      FRM_COUNT_IN_PROGRESS_INTERNSHIPS   : Frame3 ( code == "FRM_COUNT_IN_PROGRESS_INTERNSHIPS" )
      FRM_COUNT_COMPLETED_INTERNSHIPS   : Frame3 ( code == "FRM_COUNT_COMPLETED_INTERNSHIPS" )
    then
      System.out.println("   Generate FRM_DASHBOARD_INTERNSHIPS_CONTENT Rule  "+serviceToken.getUserCode());

      Frame3 frame = Frame3.builder("FRM_DASHBOARD_INTERNSHIPS_CONTENT")
                      .addTheme("THM_DASHBOARD_CONTENT_WRAPPER", ThemePosition.WRAPPER, serviceToken).end()
                      .addTheme("THM_DASHBOARD_CONTENT_NORTH", ThemePosition.NORTH, serviceToken).end()
                      .addFrame(FRM_COUNT_ALL_INTERNSHIPS, FramePosition.NORTH).end()
                      .addFrame(FRM_COUNT_AVAILABLE_INTERNSHIPS, FramePosition.NORTH).end()
                      .addFrame(FRM_COUNT_APPLIED_SHORTLISTED_INTERVIEWED_INTERNSHIPS, FramePosition.NORTH).end()
                      .addFrame(FRM_COUNT_OFFERED_INTERNSHIPS, FramePosition.NORTH).end()
                      .addFrame(FRM_COUNT_PLACED_INTERNSHIPS, FramePosition.NORTH).end()
                      .addFrame(FRM_COUNT_IN_PROGRESS_INTERNSHIPS, FramePosition.NORTH).end()
                      .addFrame(FRM_COUNT_COMPLETED_INTERNSHIPS, FramePosition.NORTH).end()
                      .build();
      frame.setRealm(serviceToken.getRealm());

      insert (frame);

      /* Process and save to cache */
      FrameUtils2.toMessage(frame, serviceToken);

end
