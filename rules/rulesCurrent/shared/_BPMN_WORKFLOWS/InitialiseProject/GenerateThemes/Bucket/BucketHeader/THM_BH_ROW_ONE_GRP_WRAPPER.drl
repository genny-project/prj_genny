rule "THM_BH_ROW_ONE_GRP_WRAPPER"
  ruleflow-group 'GenerateThemes'
  no-loop true

    when
      not  Theme (code == "THM_BH_ROW_ONE_GRP_WRAPPER")
      serviceToken : GennyToken( code == "PER_SERVICE")
    then
      System.out.println("   Generate THM_BH_ROW_ONE_GRP_WRAPPER Rule ");
      Theme theme = Theme.builder("THM_BH_ROW_ONE_GRP_WRAPPER")
                    .addAttribute()
                      .justifyContent("space-between")
                      .marginBottom(10)
                    .end()
                    .build();

      theme.setRealm(serviceToken.getRealm());
      insert(theme);
      VertxUtils.putObject(serviceToken.getRealm(), "", theme.getCode(), theme, serviceToken.getToken());
end
