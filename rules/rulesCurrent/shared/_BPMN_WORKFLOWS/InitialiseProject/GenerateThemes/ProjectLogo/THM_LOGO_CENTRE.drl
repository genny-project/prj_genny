rule "THM_LOGO_CENTRE"
  ruleflow-group 'GenerateThemes'

  no-loop true

  when
    not  Theme(code == "THM_LOGO_CENTRE")
    serviceToken : GennyToken( code == "PER_SERVICE")

  then
    System.out.println(" Generate THM_LOGO_CENTRE Rule  ");

    Theme theme = Theme.builder("THM_LOGO_CENTRE")
                  .addAttribute()
                    .fit("contain")
                    .height(100)
                    .width(100)
                  .end()
                  .build();
     theme.setRealm(serviceToken.getRealm());
    insert (theme);

    VertxUtils.putObject(serviceToken.getRealm(), "", theme.getCode(), theme, 
    serviceToken.getToken());
end 
