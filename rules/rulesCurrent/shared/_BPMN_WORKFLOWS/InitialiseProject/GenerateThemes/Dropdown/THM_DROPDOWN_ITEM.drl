rule "THM_DROPDOWN_ITEM"
  ruleflow-group 'GenerateThemes'

  no-loop true

  when
    not  Theme(code == "THM_DROPDOWN_ITEM")
    serviceToken : GennyToken( code == "PER_SERVICE")

  then
    System.out.println(" Generate THM_DROPDOWN_ITEM Rule  ");

    Theme theme = Theme.builder("THM_DROPDOWN_ITEM")
                  .addAttribute(ThemeAttributeType.PRI_IS_DROPDOWN_ITEM, true)
                  .end()
                  .build();
                  
    theme.setRealm(serviceToken.getRealm());
    insert (theme);

    VertxUtils.putObject(serviceToken.getRealm(), "", theme.getCode(), theme, 
    serviceToken.getToken());
end