package life.genny.rules;
import life.genny.qwanda.message.QDataAnswerMessage;
import life.genny.models.GennyToken;
import life.genny.rules.QRules;
import life.genny.utils.BaseEntityUtils;
import life.genny.qwanda.Answer;
import life.genny.utils.OutputParam;
import io.vertx.core.json.JsonObject;

import java.util.List;
import java.util.ArrayList;

rule "ORANGE_REMINDER_OFFERED"
    ruleflow-group 'SpecificReminder'
    no-loop true
    salience 1 
    when
	 	userToken : GennyToken (code != "PER_SERVICE" )
		serviceToken : GennyToken (code == "PER_SERVICE" )
		bucketCode : String(bucketCode.equals("Offered"))
		colourCode : String(colourCode.equals("Orange"))
	  
     then
    
		JsonObject jsonTemplates = new JsonObject();
		jsonTemplates.put("1", "ORANGE_OFFERED_REMINDER_EMAIL_TEMPLATE_APPLIED_STUDENT");
		jsonTemplates.put("2", "ORANGE_OFFERED_REMINDER_EMAIL_TEMPLATE_APPLIED_SCHOOL");
		jsonTemplates.put("3", "ORANGE_OFFERED_REMINDER_EMAIL_TEMPLATE_APPLIED_SALESTEAM");
		jsonTemplates.put("4", "ORANGE_OFFERED_REMINDER_SMS_TEMPLATE_APPLIED_STUDENT");
		System.out.println("Offered Orange Reminder Notitificaitons Template Json: " + jsonTemplates.toString());
		
	/* output.setTypeOfResult("NotificationHubTemplateFound"); */
	/* output.setResultCode(jsonTemplates.toString()); */
	/* retract(output); */
		    
 	/* System.out.println("SUCCESS -> Set notification code to APPLIED" );  */
 		
 		retract(userToken)
 		retract(serviceToken)
 		retract(bucketCode)
 		retract(colourCode)
 		
 
end
