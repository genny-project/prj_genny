package life.genny.rules;
import life.genny.qwanda.message.QEventMessage;
import life.genny.models.GennyToken;
import life.genny.utils.VertxUtils;
import io.vertx.core.json.JsonObject;
rule "DEFAULT_EVENT"
    ruleflow-group 'EventProcessing'
    no-loop
    when
		$message : QEventMessage( )
		gtoken : GennyToken ()
    then
	
		System.out.println("Processing Answer/Data : " + $message );
		
		String gotoPage = "FRM_DESKTOP";
		String key = gtoken.getSessionCode()+":page";
		VertxUtils.writeCachedJson(gtoken.getRealm(), key, gotoPage,gtoken.getToken());
		
		System.out.println("sssss :"+ key);
		
end