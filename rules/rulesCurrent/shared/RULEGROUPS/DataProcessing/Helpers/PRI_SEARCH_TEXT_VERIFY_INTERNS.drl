rule "PRI_SEARCH_TEXT_VERIFY_INTERNS"
    ruleflow-group 'DataProcessing'
        salience 500
        no-loop true
    when
 	  beUtils : BaseEntityUtils()
 	  $capabilityUtils : CapabilityUtils( )
 	  eval(beUtils.getGennyToken().hasRole("admin")) ) 
	  answer : Answer( attributeCode == "PRI_SEARCH_TEXT" && (value.startsWith("verify:interns")) )   /* this is a pure search */
	  
    then
  		System.out.println("Rule -> "+drools.getRule().getName()+" :  user=" + beUtils.getGennyToken().getUserCode()+" : "+answer);
   		retract(answer);
   		
	
			
		System.out.println("Finished sending verifications to all interns");
end
