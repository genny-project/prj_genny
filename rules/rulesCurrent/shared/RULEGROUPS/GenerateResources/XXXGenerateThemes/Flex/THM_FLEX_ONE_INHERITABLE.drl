rule "THM_FLEX_ONE_INHERITABLE"
  ruleflow-group 'GenerateThemes'

  no-loop true

  when
    not  Theme(code == "THM_FLEX_ONE_INHERITABLE")
    serviceToken : GennyToken( code == "PER_SERVICE")

  then
    System.out.println(" Generate THM_FLEX_ONE_INHERITABLE Rule  ");

    Theme theme = Theme.builder("THM_FLEX_ONE_INHERITABLE")
                  .addAttribute()
                    .flex(1)
                  .end()
                  .build();
    theme.setRealm(serviceToken.getRealm());
    insert (theme);

    VertxUtils.putObject(serviceToken.getRealm(), "", theme.getCode(), theme,
    serviceToken);
end