rule "THM_TABLE_HEADER_JUSTIFY_CONTENT"
  ruleflow-group 'GenerateThemes'

  no-loop true

  when
    not  Theme(code == "THM_TABLE_HEADER_JUSTIFY_CONTENT")
    serviceToken : GennyToken( code == "PER_SERVICE")

  then
    System.out.println(" Generate THM_TABLE_HEADER_JUSTIFY_CONTENT Rule  ");

    Theme theme = Theme.builder("THM_TABLE_HEADER_JUSTIFY_CONTENT")
                  .addAttribute()
                   .justifyContent("center")
                   .alignItems("center")
                  .end()
                  .addAttribute(ThemeAttributeType.PRI_IS_INHERITABLE, true).end()
                  .build();
  
    
    theme.setRealm(serviceToken.getRealm());
    insert (theme);

    VertxUtils.putObject(serviceToken.getRealm(), "", theme.getCode(), theme, 
    serviceToken);
end