package life.genny.rules;
import life.genny.rules.QRules;

rule "Trigger Tab"
	when
		rules: QRules(  isState("TRIGGER_SEND_TAB") &&
						!isState("LOOP_TRIGGER_SEND_TAB"))
	 then
	 	rules.header();
		rules.setState("LOOP_TRIGGER_SEND_TAB");

		rules.setState("GENERATE_TAB_FRAMES");
		rules.footer();
end
