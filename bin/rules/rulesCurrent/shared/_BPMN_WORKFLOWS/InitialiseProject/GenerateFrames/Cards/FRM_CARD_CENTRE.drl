rule "FRM_CARD_CENTRE"
    ruleflow-group 'GenerateFrames'

    when
      not  Frame3(code == "FRM_CARD_CENTRE")
      serviceToken : GennyToken( code == "PER_SERVICE")
      FRM_DUMMY : Frame3 ( code == "FRM_DUMMY" )
      FRM_NAME : Frame3 ( code == "FRM_NAME" )
      FRM_EMAIL : Frame3 ( code == "FRM_EMAIL" )
      FRM_MOBILE : Frame3 ( code == "FRM_MOBILE" )
      FRM_DOB : Frame3 ( code == "FRM_DOB" )
    then
     System.out.println("   Generate FRM_CARD_CENTRE Rule  "+serviceToken.getUserCode());

 	 Frame3 frame = Frame3.builder("FRM_CARD_CENTRE")
                                .addTheme("THM_CARD_CENTRE",serviceToken).end()
                                /*.question("QUE_NAME_TWO")*/
                                .addFrame(FRM_NAME, FramePosition.NORTH).end()
                                .addFrame(FRM_EMAIL, FramePosition.NORTH).end()
                                .addFrame(FRM_MOBILE, FramePosition.NORTH).end()
                                .addFrame(FRM_DOB, FramePosition.NORTH).end()
                                .build();
                
	frame.setRealm(serviceToken.getRealm());
						
	insert (frame);
	
	/* Process and save to cache */
	FrameUtils2.toMessage(frame, serviceToken);

end
