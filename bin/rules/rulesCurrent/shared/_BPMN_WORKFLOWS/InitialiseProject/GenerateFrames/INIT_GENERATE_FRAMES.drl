import org.kie.api.runtime.process.WorkflowProcessInstance;

rule "INIT_GENERATE_FRAMES"
    ruleflow-group 'GenerateFrames'
	salience 10000
    when
		$process : WorkflowProcessInstance( )  
   		not  GennyToken (code == "PER_SERVICE")
  		
     then
     	System.out.println("   Setup GennyToken for Generate Frames");
     	GennyToken serviceToken  = (GennyToken)$process.getVariable("serviceToken");
 		insert(serviceToken);
  
end
